<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:paddingHorizontal="13dp"
    android:paddingVertical="8dp"
    android:layout_height="61dp"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/ivTrack"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        style="@style/tv_text"
        android:id="@+id/tvName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginEnd="9dp"
        app:layout_constraintStart_toEndOf="@id/ivTrack"
        app:layout_constraintTop_toTopOf="@id/ivTrack"
        app:layout_constraintBottom_toTopOf="@id/tvNameArtist"
        android:fontFamily="@font/ys_display_regular"
        tools:text="string string string string string string string string string string string string string string string string string string string string string string string string string string string "
        android:textFontWeight="400"
        android:layout_marginStart="8dp"
        android:textSize="16sp"
        android:ellipsize="end"
        android:maxLines="1"
        app:layout_constraintEnd_toStartOf="@id/btnGo"
        />
    <TextView
        style="@style/tv_text2"
        android:id="@+id/tvNameArtist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toBottomOf="@id/tvName"
        app:layout_constraintStart_toEndOf="@id/ivTrack"
        app:layout_constraintBottom_toBottomOf="@id/ivTrack"
        app:layout_constraintEnd_toStartOf="@id/dot"
        android:fontFamily="@font/ys_display_regular"
        android:textSize="11sp"
        tools:text="12332112332112332111231233211233123123321123321123321"
        android:layout_marginStart="8dp"
        app:layout_constraintHorizontal_chainStyle="packed"
        android:ellipsize="end"
        android:maxLines="1"
        />

    <ImageView
        android:id="@+id/dot"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:background="@drawable/img_dot"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/tvNameArtist"
        app:layout_constraintEnd_toStartOf="@id/tvDuration"
        app:layout_constraintStart_toEndOf="@id/tvNameArtist"
        app:layout_constraintTop_toTopOf="@+id/tvNameArtist" />
    <TextView
        style="@style/tv_text2"
        android:id="@+id/tvDuration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_regular"
        app:layout_constraintStart_toEndOf="@id/dot"
        app:layout_constraintTop_toTopOf="@id/tvNameArtist"
        app:layout_constraintBottom_toBottomOf="@id/tvNameArtist"
        tools:text="22:22"
        android:textSize="11sp"
        app:layout_constraintEnd_toStartOf="@id/btnGo"
        android:layout_gravity="center_vertical"
        />
    <ImageView
        android:id="@+id/btnGo"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/arrow_small"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:importantForAccessibility="no" />

</androidx.constraintlayout.widget.ConstraintLayout>